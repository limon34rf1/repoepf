{
  "version": 2,
  "builds": [{
    "src": "public/index.html",
    "use": "@vercel/static"
  }],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}